#include<bits/stdc++.h>
#define ll long long 
const ll mod=1e9+7;
const int maxn=1e6+300;
using namespace std;
 
int main() 
{ 
   #ifndef ONLINE_JUDGE
      freopen("input.txt","r",stdin);
    #endif
   int n;
   cin>>n;
   int ax,ay,bx,by,cx,cy;
   cin>>ax>>ay;
   cin>>bx>>by;
   cin>>cx>>cy;
   bool b=true;
   if((bx<=ax)and(cx>=ax))
   {
   	b=false;
   }
   if((bx>=ax)and(cx<=ax))
   {
   	b=false;
   }
   if((by<=ay)and(cy>=ay))
   {
   	b=false;
   }
   if((by>=ay)and(cy<=ay))
   {
   	b=false;
   }
   if(!b)
   {
   	cout<<"NO"<<endl;
   }
   else
   {
   	cout<<"YES"<<endl;
   }
   
}
